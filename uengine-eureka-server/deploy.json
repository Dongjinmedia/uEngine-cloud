{
  "id": "/eureka-server",
  "cmd": null,
  "cpus": 0.5,
  "mem": 512,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles":["slave_public"],
  "container": {
    "type": "DOCKER",
    "docker": {
      "forcePullImage": true,
      "image": "gitlab.pas-mini.io:5000/uengine-eureka-server:v1",
      "parameters": [],
      "privileged": false
    },
    "volumes": [],
    "portMappings": [
      {
        "containerPort": 8761,
        "hostPort": 0,
        "labels": {},
        "protocol": "tcp",
        "servicePort": 10001
      }
    ]
  },
  "env": {
    "DEPLOYMENT": "BLUE"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "path": "/",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 20,
      "delaySeconds": 15
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external,internal",
    "HAPROXY_0_VHOST": "eureka-server.pas-mini.io"
  },
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "portDefinitions": []
}